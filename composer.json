{
	"name": "medinaproduction/laravel-toolkit",
	"description": "MedinaProduction Laravel Toolkit Package",
	"type": "library",
	"keywords": ["laravel"],
	"license": "MIT",
	"authors": [
		{
			"name": "Edward Karlsson",
			"email": "edward@medinaproduction.se"
		}
	],
	"require": {
		"php": "^8.1",
		"laravel/framework": "^8.0|^9.0|^10.0"
    },
	"require-dev": {
		"fakerphp/faker": "^1.9",
		"mockery/mockery": "^1.4",
		"phpunit/phpunit": "^9.5",
		"orchestra/testbench": "^7.13"
	},
	"autoload": {
		"psr-4": {
			"MedinaProduction\\Toolkit\\": "src/",
			"MedinaProduction\\Toolkit\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit"
	},
	"extra": {
		"laravel": {
			"providers": [
				"MedinaProduction\\Toolkit\\ServiceProvider"
			]
		}
	}
}
